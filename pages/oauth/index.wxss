
.auth_header {
    padding: 90rpx 50rpx 90rpx 50rpx;
    text-align: center;
    height: 300rpx;
    line-height: 450rpx;
    border-bottom: 1px solid #eee;
}
.auth_header image {
    width: 200rpx;
    height: 200rpx;
}

.auth_content {
   padding: 50rpx 90rpx;
}

.auth_content text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
}

.auth_bottom {
    border-radius: 80rpx;
    margin: 0 50rpx;
    font-size: 35rpx;
}